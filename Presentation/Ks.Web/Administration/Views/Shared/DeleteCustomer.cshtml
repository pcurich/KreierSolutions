@model Ks.Web.Framework.Mvc.DeleteConfirmationModel
@using (Html.BeginForm(Model.ActionName, Model.ControllerName, new { id = Model.Id }))
{
    @Html.AntiForgeryToken()
    <div>
        <p>
            Antes de Desactivar al asociado verifique siguientes acciones:
            <ul>
                <li>Cancele todos los apoyos economicos vigentes que tenga</li>
            </ul>
            Al Desactivar a asociado este perdera todas las apoetaciones realizadas asi como tambien los beneficios
        </p>
        <p>Desea continuar?</p>
        <div class="form-actions">
            <span id="" class="k-button" onclick="$('#@Model.WindowId').data('kendoWindow').close();">@T("Admin.Common.NoCancel")</span>
            <input type="submit" class="k-button" value="@T("Admin.Common.Yes")" />
        </div>
    </div>
}
